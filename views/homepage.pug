doctype html
html
    head
        title= Title
        link(rel='stylesheet', href='/stylesheets/index.css') 
    body
        h1 #{role}: #{userID}

        if !courses
            p No Result for #{userID}
        else if courses.length > 0
            table
                thead
                    tr
                        each key in Object.keys(courses[0])
                            th= key
                tbody 
                    each course in courses
                        tr
                            each val in Object.values(course)
                                td= val